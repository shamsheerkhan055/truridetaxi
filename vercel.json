{
    "builds": [
      {
        "src": "package.json",
        
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    
      "routes": [
    {
      "src": "/[^.]+",
      "dest": "/"
    }
  ],
      
        "rewrites": [
          {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
          }
        ]
      
    }

    
  