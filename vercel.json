{
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    
      "routes": [
        { "src": "/", "dest": "/" },
        { "src": "/features", "dest": "/" },
        { "src": "/aboutus", "dest": "/" },
        { "src": "/howitworks", "dest": "/" },
        { "src": "/testimonials", "dest": "/" },
        { "src": "/contactus", "dest": "/" },
        { "src": "/gettheapp", "dest": "/" }
        
      ]
    }
  